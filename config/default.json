{
  "docker": { "socketPath": "/var/run/docker.sock"},
  "secret_folder": {
    "testing": "./secrets",
    "testing_nested": "./secrets/nested"
  },
  "logging": {
    "transport": {
      "console": [
        {
          "level": "info",
          "handleExceptions": true,
          "json": false,
          "colorize": true
        }
      ]
    },
    "exitOnError": false,
    "formatters": [
      "timestamp",
      "splat",
      "prettyPrint"
    ]
  },
  "update_interval": 10000,
  "update_detection_interval": 1000,
  "max_versions": 1000,
  "secret_wait_time": 10000,
  "service_wait_time": 30000,
  "md5_check_interval": 5000
}