{
  "docker": { "socketPath": "/var/run/docker.sock"},
  "secret_folder": {
    "testing": "./test_secrets",
    "testing_nested": "./test_secrets/nested"
  },
  "logging": {
    "transport": {
      "console": [
        {
          "level": "info",
          "handleExceptions": true,
          "json": false,
          "colorize": true
        }
      ]
    },
    "exitOnError": false,
    "formatters": [
      "timestamp",
      "splat",
      "prettyPrint",
      "ecs"
    ]
  },
  "updateInterval": 1000
}